<resource-manager-builder xmlns="http://org.eclipse.ptp/rm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="resource_manager_type.xsd"
	name="IBM Parallel Environment Advanced Mode">
	<!-- Make sure XML editor line length is set long enough (160) to avoid wrapping text so messages don't get wrapped when reformatting -->
	<control-data>
		<!-- Required for debugger to determine number of processes to display -->
		<attribute name="mpiNumberOfProcesses" type="integer">
			<link-value-to>MP_PROCS</link-value-to>
		</attribute>
	
		<attribute name="MP_ACK_THRESH" type="string">
			<description>Acknowledgment threshold:</description>
			<tooltip>Specify packet acknowledgment threshold </tooltip>
		</attribute>

		<attribute name="MP_ADAPTER_USE" type="string">
			<description>Exclusive adapter use:</description>
			<tooltip>Specify how node's adapter should be used</tooltip>
		</attribute>

		<attribute name="MP_BINDPROC" type="string">
			<description>Bind tasks to first CPU:</description>
			<tooltip>Bind tasks to first logical CPU. Use for OpenMP Only</tooltip>
		</attribute>

		<attribute name="MP_BULK_MIN_MSG_SIZE" type="string">
			<description>Minimum bulk message size:</description>
			<tooltip>Specify minimum message size for messages using bulk transfer (Default: 153600)</tooltip>
		</attribute>

		<attribute name="MP_CC_SCRATCH_BUF" type="string">
			<description>Fastest collectives:</description>
			<tooltip>Specify if fastest collective algorithm is used</tooltip>
		</attribute>

		<attribute name="MP_CLOCK_SOURCE" type="string">
			<description>Clock source:</description>
			<tooltip>Specify if high performance switch is clock source</tooltip>
		</attribute>

		<attribute name="MP_CMDFILE" type="string">
			<description>Command file:</description>
			<tooltip>Specify script to load nodes in partition</tooltip>
		</attribute>

		<attribute name="MP_COLLECTIVE_GROUPS" type="string">
			<description>CAU groups:</description>
			<tooltip>Specify the number of CAU groups (Default: 0)</tooltip>
		</attribute>

		<attribute name="MP_CONFIG" type="string">
			<description>Use PE release level:</description>
			<tooltip>Specify the PE release level to use</tooltip>
		</attribute>

		<attribute name="MP_COREDIR" type="string">
			<description>Core file directory:</description>
			<tooltip>Specify directory where core files are stored.</tooltip>
		</attribute>

		<attribute name="MP_COREFILE_FORMAT" type="string">
			<description>Corefile format:</description>
			<tooltip>Specify 'STDERR' to write core data to stderr, blank for standard core file or name of lightweight corefile</tooltip>
		</attribute>

		<attribute name="MP_COREFILE_SIGTERM" type="string">
			<description>Corefile on SIGTERM:</description>
			<tooltip>Select to create corefile if application gets SIGTERM</tooltip>
		</attribute>

		<attribute name="MP_COSCHEDULER_VERSION" type="string">
			<description>Use runqueue co-scheduler:</description>
			<tooltip>Specifies if POE runqueue co-scheduler is used</tooltip>
		</attribute>

		<attribute name="MP_CPU_USE" type="string">
			<description>Exclusive CPU use:</description>
			<tooltip>Specify how node's CPU should be used</tooltip>
		</attribute>

		<attribute name="MP_CSS_INTERRUPT" type="string">
			<description>Packets generate interrupts:</description>
			<tooltip>Specify if arriving packets generate interrupts (Default:no)</tooltip>
		</attribute>

		<attribute name="MP_DEVTYPE" type="string">
			<description>Device type class:</description>
			<tooltip>Select 'hfi' to use HFI device type, blank otherwise</tooltip>
		</attribute>

		<attribute name="MP_EAGER_LIMIT" type="string">
			<description>Eager limit:</description>
			<tooltip>Specify rendezvous protocol message size threshold (Default:4096)</tooltip>
		</attribute>

		<attribute name="MP_EUIDEVELOP" type="string">
			<description>MPI parameter checking:</description>
			<tooltip>Specify level of MPI parameter checking. (Default: no)</tooltip>
		</attribute>

		<attribute name="MP_EUIDEVICE" type="string">
			<description>Adapter:</description>
			<tooltip>Specify adapter to use for message passing</tooltip>
		</attribute>

		<attribute name="MP_EUILIB" type="string">
			<description>Communications subsystem:</description>
			<tooltip>Communication subsystem to be used (Default: ip)</tooltip>
		</attribute>

		<attribute name="MP_EUILIBPATH" type="string">
			<description>Library path:</description>
			<tooltip>Specify path to message passing and communication libraries (Default: /opt/ibmhpc/pe1100/ppe.poe/lib)</tooltip>
		</attribute>

		<attribute name="MP_FIFO_ROUTE_MODE" type="string">
			<description>FIFO routing mode:</description>
			<tooltip>Routing mode to be used for FIFO operations (Default:hw_direct)</tooltip>
		</attribute>

		<attribute name="MP_FORCED_INTERRUPTS_ENABLED" type="string">
			<description>Use forced interrupts:</description>
			<tooltip>Use forced interrupts for rondezvous message in RDMA transfer (Default: no)</tooltip>
		</attribute>

		<attribute name="MP_HINTS_FILTERED" type="string">
			<description>Hints filtered:</description>
			<tooltip>Specify if MPI info objects reject hints (Default: no)</tooltip>
		</attribute>

		<attribute name="MP_HOSTFILE" type="string">
			<description>Host list file:</description>
			<tooltip>Specify the name of the hostfile that maps tasks to nodes.</tooltip>
		</attribute>

		<attribute name="MP_IMM_SEND_BUFFERS" type="string">
			<description>Immediate send buffers:</description>
			<tooltip>Specify the number of immediate send buffers (Default: 1)</tooltip>
		</attribute>

		<attribute name="MP_INFOLEVEL" type="string">
			<description>PE diagnostic message level:</description>
			<tooltip>Specify PE diagnostic message level. (Default: 1)</tooltip>
		</attribute>

		<attribute name="MP_INSTANCES" type="string">
			<description>Window instances:</description>
			<tooltip>Specify number of user space windows or IP addresses to use (Default: 1)</tooltip>
		</attribute>

		<attribute name="MP_IOAGENT_CNT" type="string">
			<description>I/O agent count:</description>
			<tooltip>Specify the number I/O agents.</tooltip>
		</attribute>

		<attribute name="MP_IONODEFILE" type="string">
			<description>MPI I/O Node List:</description>
			<tooltip>Specify the file listing nodes performing parallel I/O</tooltip>
		</attribute>

		<attribute name="MP_IOTASKLIST" type="string">
			<description>I/O agents tasks:</description>
			<tooltip>Specify a ':' separated list of I/O agent tasks</tooltip>
		</attribute>

		<attribute name="MP_IO_BUFFER_SIZE" type="string">
			<description>MPI_IO buffer size:</description>
			<tooltip>Specify size of MPI_IO buffer </tooltip>
		</attribute>

		<attribute name="MP_IO_ERRLOG" type="string">
			<description>Create I/O error log:</description>
			<tooltip>Specify if I/O error logging is enabled (Default: no)</tooltip>
		</attribute>

		<attribute name="MP_LABELIO" type="string">
			<description>Label STDIO with task number:</description>
			<tooltip>Prefix stdout and stderr for each task with task number.</tooltip>
		</attribute>

		<attribute name="MP_MC_INET_ADDR_LEN" type="string">
			<description>Number of multicast addresses:</description>
			<tooltip>Specify size of multicase address range (Default: 16)</tooltip>
		</attribute>

		<attribute name="MP_MC_INET_BASE_ADDR" type="string">
			<description>Base multicast IP address:</description>
			<tooltip>Specify the base IPv4 multicast address (Default: 224.3.0.1)</tooltip>
		</attribute>

		<attribute name="MP_MC_INET_PORT" type="string" min="1024" max="65535">
			<description>Multicast port number:</description>
			<tooltip>Specify port number to use for multicasting (Default: 2008)</tooltip>
		</attribute>
		
		<attribute name="MP_MPILIB" type="string">
			<description>MPI library:</description>
			<tooltip>MPI library used (mpich2 or shim)</tooltip>
		</attribute>

		<attribute name="MP_MSG_API" type="string">
			<description>Message passing API:</description>
			<tooltip>Specify message passing API(s) used by application (Default:mpi)</tooltip>
		</attribute>

		<attribute name="MP_MSG_ENVELOPE_BUF" type="string">
			<description>Envelope buffer size:</description>
			<tooltip>Specify size of message envelope buffer (Default 8388608)</tooltip>
		</attribute>

		<attribute name="MP_NEWJOB" type="string">
			<description>Keep job partition:</description>
			<tooltip>Specifies if partition manager maintains partition for multiple job steps</tooltip>
		</attribute>

		<attribute name="MP_NODES" type="string">
			<description>Number of nodes:</description>
			<tooltip>Specify the number of nodes to run application.</tooltip>
		</attribute>

		<attribute name="MP_PGMMODEL" type="string">
			<description>Programming model:</description>
			<tooltip>Specify programming model (Default: spmd)</tooltip>
		</attribute>

		<attribute name="MP_PMDLOG" type="string">
			<description>Generate PMD diagnostic log:</description>
			<tooltip>Generate a PMD log for diagnostic purposes. (Default: no)</tooltip>
		</attribute>

		<attribute name="MP_PMDLOG_DIR" type="string">
			<description>PMD log directory:</description>
			<tooltip>Specify directory where PMD log is stored. (Default: /tmp)</tooltip>
		</attribute>

		<attribute name="MP_PMDSUFFIX" type="string">
			<description>Alternate PMD service suffix:</description>
			<tooltip>Specify the suffix for the PMD service name</tooltip>
		</attribute>

		<attribute name="MP_POLLING_INTERVAL" type="string">
			<description>MPI polling interval:</description>
			<tooltip>Specify PE polling interval (Default: 400000)</tooltip>
		</attribute>

		<attribute name="MP_PRINTENV" type="string">
			<description>Print environment:</description>
			<tooltip>Print MPI environment variables for all tasks (Default: no)</tooltip>
		</attribute>

		<attribute name="MP_PRIORITY" type="string">
			<description>Co-scheduler priority:</description>
			<tooltip>Specify co-scheduler priority class name </tooltip>
		</attribute>

		<attribute name="MP_PRIORITY_LOG" type="string">
			<description>Create co-scheduler log:</description>
			<tooltip>Specify if co-scheduler log should be created (Default: no)</tooltip>
		</attribute>

		<attribute name="MP_PRIORITY_LOG_DIR" type="string">
			<description>Coscheduler log directory:</description>
			<tooltip>Specify directory where co-scheduler log is stored. (Default: /tmp)</tooltip>
		</attribute>

		<attribute name="MP_PRIORITY_LOG_NAME" type="string">
			<description>Co-scheduler log name:</description>
			<tooltip>Specify name of co-scheduler log (Default: pmadjpri.log)</tooltip>
		</attribute>

		<attribute name="MP_PRIORITY_NTP" type="string">
			<description>Co-scheduler disables NTP:</description>
			<tooltip>Specify if PE co-scheduler disables NTP during adjustment period (Default: no)</tooltip>
		</attribute>

		<attribute name="MP_PROCS" type="string">
			<description>Number of application tasks:</description>
			<tooltip>Specify the number of application tasks. (Default: 1)</tooltip>
		</attribute>

		<attribute name="MP_PROFDIR" type="string">
			<description>GMON file directory:</description>
			<tooltip>Specify directory where GMON files are stored.</tooltip>
		</attribute>

		<attribute name="MP_PULSE" type="string">
			<description>Pulse interval:</description>
			<tooltip>Specify the interval at which PE checks remote nodes (Default: 600)</tooltip>
		</attribute>

		<attribute name="MP_RC_MAX_QP" type="string">
			<description>Maximum QPairs:</description>
			<tooltip>Specify the maximum number of QPairs (Default: 8156)</tooltip>
		</attribute>

		<attribute name="MP_RC_USE_LMC" type="string">
			<description>Use LMC:</description>
			<tooltip>Use lid mask control (LMC) (Default: no) </tooltip>
		</attribute>

		<attribute name="MP_RDMA_ROUTE_MODE" type="string">
			<description>RDMA routing mode:</description>
			<tooltip>Routing mode to be used for RDMA operations (Default:hw_direct)</tooltip>
		</attribute>

		<attribute name="MP_REMOTEDIR" type="string">
			<description>Remote directory script:</description>
			<tooltip>Specify script to echo name of current directory to be used on remote nodes</tooltip>
		</attribute>
		
		<attribute name="MP_RESD" type="string">
			<description>Use resource manager:</description>
			<tooltip>Use resource manager to handle resource allocations</tooltip>
		</attribute>

		<attribute name="MP_RETRANSMIT_INTERVAL" type="string">
			<description>MPI retransmit interval:</description>
			<tooltip>Specify interval to check if retransmit required</tooltip>
		</attribute>

		<attribute name="MP_RETRY" type="string">
			<description>Job submission timeout:</description>
			<tooltip>"Specify job submission timeout ( &lt;0: wait indefinitely, 0: no wait, &gt;0, wait specified interval) (Default: 0)</tooltip>
		</attribute>

		<attribute name="MP_REXMIT_BUF_CNT" type="string">
			<description>Retransmit buffers:</description>
			<tooltip>Specify number of retransmit buffers per task (Default: 128)</tooltip>
		</attribute>

		<attribute name="MP_REXMIT_BUF_SIZE" type="string">
			<description>Maximum PAMI/LAPI buffered message:</description>
			<tooltip>Specify maximum LAPI buffer size (Default: 16352)</tooltip>
		</attribute>

		<attribute name="MP_RMFILE" type="string">
			<description>Node allocation file:</description>
			<tooltip>Specify resource manager command file for node allocation</tooltip>
		</attribute>

		<attribute name="MP_RMPOOL" type="string">
			<description>Node allocation pool:</description>
			<tooltip>"Specify name of node allocation pool</tooltip>
		</attribute>

		<attribute name="MP_RM_TIMEOUT" type="string">
			<description>Resource mgr connect timeout:</description>
			<tooltip>"Specify resource manager connection timeout (&lt;0: wait indefinitely, 0: no wait, &gt;0, wait specified interval) (Default:30)</tooltip>
		</attribute>
		
		<attribute name="MP_RMA_PENDING" type="string">
			<description>Maximum outstanding RMA requests:</description>
			<tooltip>"Maximum outstanding RMA requests. Limits number of PAMI_Request objects allocated</tooltip>
		</attribute>

		<attribute name="MP_SAVEHOSTFILE" type="string">
			<description>Generated host file:</description>
			<tooltip>Specify path to saved hostfile generated by partition manager</tooltip>
		</attribute>

		<attribute name="MP_SAVE_RMFILE" type="string">
			<description>Generated job command file:</description>
			<tooltip>Specify path to saved job command file generated by partition manager</tooltip>
		</attribute>

		<attribute name="MP_SHARED_MEMORY" type="string">
			<description>Use shared memory:</description>
			<tooltip>Specify if shared memory is used for communication (Default:yes)</tooltip>
		</attribute>

		<attribute name="MP_SHMCC_EXCLUDE_LIST" type="string">
			<description>Shared memory exclusions:</description>
			<tooltip>Specify list of MPI collecttives excluded from using shared memory</tooltip>
		</attribute>

		<attribute name="MP_SHMEM_PT2PT" type="string">
			<description>Point to point shared memory:</description>
			<tooltip>Intranode point to point communication uses shared memory</tooltip>
		</attribute>

		<attribute name="MP_SINGLE_THREAD" type="string">
			<description>Single MPI thread:</description>
			<tooltip>Specify if application has single thread with MPI calls (Default: no)</tooltip>
		</attribute>

		<attribute name="MP_STATISTICS" type="string">
			<description>MPI statistics:</description>
			<tooltip>Obtain communication statistics for user space jobs.</tooltip>
		</attribute>

		<attribute name="MP_STDINMODE" type="string">
			<description>STDIN processing mode:</description>
			<tooltip>Specify how stdin is handled. (Default: all)</tooltip>
		</attribute>

		<attribute name="MP_STDOUTMODE" type="string">
			<description>STDOUT/STDERR processing mode:</description>
			<tooltip>Specify how stdout and stderr are handled. (Default:unordered)</tooltip>
		</attribute>

		<attribute name="MP_TASKS_PER_NODE" type="string">
			<description>Number of tasks per node:</description>
			<tooltip>Specify the number application tasks per node.</tooltip>
		</attribute>

		<attribute name="MP_TASK_AFFINITY" type="string">
			<description>Task affinity:</description>
			<tooltip>Attach application tasks to system rset for task affinity</tooltip>
		</attribute>

		<attribute name="MP_THREAD_STACKSIZE" type="string">
			<description>Additional MPI thread stack size:</description>
			<tooltip>Specify additional stack size for MPI service thread (Default: 0)</tooltip>
		</attribute>

		<attribute name="MP_TIMEOUT" type="string">
			<description>Connection timeout:</description>
			<tooltip>Specify timeout limit for connecting to remote nodes (Default: 150)</tooltip>
		</attribute>

		<attribute name="MP_TIMEOUT" type="string">
			<description>POE connection timeout:</description>
			<tooltip>"Specify POE connection timeout</tooltip>
		</attribute>

		<attribute name="MP_TLP_REQUIRED" type="string">
			<description>Large pages required:</description>
			<tooltip>Specifies if POE shuld check if application was compiled for large pages</tooltip>
		</attribute>

		<attribute name="MP_UDP_PACKET_SIZE" type="string">
			<description>UDP packet size:</description>
			<tooltip>Specify UDP packet size</tooltip>
		</attribute>

		<attribute name="MP_USE_BULK_XFER" type="string">
			<description>Use bulk transfer:</description>
			<tooltip>Use adapter bult data transfer mechanisms (Default: no)</tooltip>
		</attribute>

		<attribute name="MP_USE_MC" type="string">
			<description>Use hardware multicast:</description>
			<tooltip>Specifies if hardware multicast feature is used (Default:no)</tooltip>
		</attribute>

		<attribute name="MP_WAIT_MODE" type="string">
			<description>Wait mode:</description>
			<tooltip>Specify thread behavior when waiting for messages (Default:poll) </tooltip>
		</attribute>

		<attribute name="MP_WORLD_SIZES" type="string">
			<description>MPI world sizes:</description>
			<tooltip>Specify a ':' separated list of MPI world sizes</tooltip>
		</attribute>

		<attribute name="SCI_DEBUG_FANOUT" type="string">
			<description>SCI fanout:</description>
			<tooltip>Specify fanout value for SCI agent tree (Default: 32)</tooltip>
		</attribute>


