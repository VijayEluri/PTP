			<environment name="MP_IMM_SEND_BUFFERS" value="${ptp_rm:MP_IMM_SEND_BUFFERS#value}" />
			<environment name="MP_INFOLEVEL" value="${ptp_rm:MP_INFOLEVEL#value}" />
